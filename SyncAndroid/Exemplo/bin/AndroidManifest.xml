<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.exemplo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="8" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.exemplo.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.exemplo.ImportacaoActivity"
            android:label="@string/title_activity_importacao" >
        </activity>
        <activity
            android:name="com.example.exemplo.ExportacaoActivity"
            android:label="@string/title_activity_exportacao" >
        </activity>
        <activity
            android:name="com.example.exemplo.AlertaActivity"
            android:label="@string/title_activity_alerta" >
        </activity>
    </application>
      <uses-permission android:name="android.permission.INTERNET" >
    </uses-permission>

</manifest>